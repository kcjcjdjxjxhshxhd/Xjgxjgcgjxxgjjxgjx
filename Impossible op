-- blink
coroutine.wrap(function()
    while true do
        wait(70)
        game.ReplicatedStorage.GameData.LatestRoom.Changed:Wait()
        wait(0)
        
local cue2 = Instance.new("Sound")
	cue2.Parent = game.Workspace
	cue2.Name = "Spawn"
	cue2.SoundId = "rbxassetid://7053083974"
	cue2.Volume = 5
	cue2.PlaybackSpeed = 1
	cue2:Play()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(0, 0, 0)
game.Lighting.MainColorCorrection.Contrast = 10
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
local TW = TweenService:Create(game.Lighting.MainColorCorrection, TweenInfo.new(5),{TintColor = Color3.fromRGB(209, 3, 255)})
TW:Play()
local cue2 = Instance.new("Sound")
	cue2.Parent = game.Workspace
	cue2.Name = "Spawn"
	cue2.SoundId = "rbxassetid://165969964"
	cue2.Volume = 1
	cue2.PlaybackSpeed = 2
	cue2:Play()
loadstring(game:HttpGet("https://github.com/PABMAXICHAC/doors-monsters-scripts/raw/main/blinkcrux"))()
local cue = Instance.new("Sound")
	cue.Parent = game.Workspace
	cue.Name = "Spawn"
	cue.SoundId = "rbxassetid://175024455"
	cue.Volume = 1
	cue.PlaybackSpeed = 1.2
	cue:Play()
game.Lighting.MainColorCorrection.TintColor = Color3.fromRGB(255, 107, 8)
game.Lighting.MainColorCorrection.Contrast = 10
local tween = game:GetService("TweenService")
tween:Create(game.Lighting.MainColorCorrection, TweenInfo.new(2.5), {Contrast = 0}):Play()
local TweenService = game:GetService("TweenService")
